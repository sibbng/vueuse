{
  "name": "@vueuse/integrations",
  "version": "6.7.3",
  "description": "Integration wrappers for utility libraries",
  "keywords": [
    "vue",
    "vue-use",
    "utils"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vueuse/vueuse.git"
  },
  "funding": "https://github.com/sponsors/antfu",
  "author": "Anthony Fu <https://github.com/antfu>",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./useAxios": {
      "import": "./dist/useAxios.mjs",
      "require": "./dist/useAxios.cjs"
    },
    "./useCookies": {
      "import": "./dist/useCookies.mjs",
      "require": "./dist/useCookies.cjs"
    },
    "./useDrauu": {
      "import": "./dist/useDrauu.mjs",
      "require": "./dist/useDrauu.cjs"
    },
    "./useFocusTrap": {
      "import": "./dist/useFocusTrap.mjs",
      "require": "./dist/useFocusTrap.cjs"
    },
    "./useFocusTrap/component": {
      "import": "./dist/useFocusTrap/component.mjs",
      "require": "./dist/useFocusTrap/component.cjs"
    },
    "./useFuse": {
      "import": "./dist/useFuse.mjs",
      "require": "./dist/useFuse.cjs"
    },
    "./useJwt": {
      "import": "./dist/useJwt.mjs",
      "require": "./dist/useJwt.cjs"
    },
    "./useNProgress": {
      "import": "./dist/useNProgress.mjs",
      "require": "./dist/useNProgress.cjs"
    },
    "./useQRCode": {
      "import": "./dist/useQRCode.mjs",
      "require": "./dist/useQRCode.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/vueuse/vueuse/issues"
  },
  "homepage": "https://github.com/vueuse/vueuse/tree/main/packages/integrations#readme",
  "dependencies": {
    "@vueuse/core": "workspace:*",
    "vue-demi": "*"
  },
  "optionalDependencies": {
    "axios": "^0.23.0",
    "drauu": "^0.2.1",
    "focus-trap": "^6.7.1",
    "fuse.js": "^6.4.6",
    "jwt-decode": "^3.1.2",
    "nprogress": "^0.2.0",
    "qrcode": "^1.4.4",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.1",
    "@types/universal-cookie": "^3.0.0",
    "nprogress": "^0.2.0",
    "qrcode": "^1.4.4",
    "universal-cookie": "^4.0.4"
  }
}
